{
    "name": "swag/migration-magento",
    "description": "Magento 1 Profiles for Shopware Migration Assistant",
    "version": "1.2.1",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "shopware AG"
        }
    ],
    "require": {
        "shopware/core": "~6.2.0",
        "swag/migration-assistant": "~1.0"
    },
    "extra": {
        "shopware-plugin-class": "Swag\\MigrationMagento\\SwagMigrationMagento",
        "copyright": "(c) by shopware AG",
        "label": {
            "de-DE": "Magento 1 Profil f√ºr den Migrationsassistenten",
            "en-GB": "Magento 1 profile for the Migration Assistant"
        }
    },
    "autoload": {
        "psr-4": {
            "Swag\\MigrationMagento\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\MigrationMagento\\Test\\": "tests/",
            "SwagMigrationAssistant\\": "../SwagMigrationAssistant"
        }
    },
    "suggest": {
        "ext-sodium": "Allows you to use the argon2id13 encoder to migrate customer passwords of Magento 2."
    }
}
